{
    "name": "product-luncher",
    "version": "1.0.0",
    "description": "E-Commerce AI Launch Suite - Multi-agent e-commerce platform",
    "private": true,
    "type": "module",
    "main": "src/server.js",
    "scripts": {
        "dev": "node --watch src/server.js",
        "start": "node src/server.js",
        "vercel-build": "npx prisma generate && echo 'Build complete with Prisma'",
        "build": "npx prisma generate && echo 'Build complete with Prisma'",
        "test": "echo 'Tests to be implemented'",
        "lint": "eslint src --ext .js",
        "lint:fix": "eslint src --ext .js --fix",
        "db:migrate": "npx prisma migrate dev",
        "db:seed": "node src/seed.js",
        "db:studio": "npx prisma studio",
        "setup:shopify": "node src/scripts/setup-shopify.js",
        "setup:webhooks": "node src/scripts/setup-webhooks.js",

        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "docker:logs": "docker-compose logs -f"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.9.1",
        "@prisma/client": "^5.7.1",
        "@sendgrid/mail": "^8.0.1",
        "@sentry/integrations": "^7.88.0",
        "@sentry/node": "^7.88.0",
        "@sentry/profiling-node": "^10.5.0",
        "axios": "^1.11.0",
        "bcryptjs": "^2.4.3",
        "bullmq": "^5.0.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.0.1",
        "facebook-nodejs-business-sdk": "^18.0.4",
        "helmet": "^7.1.0",
        "ioredis": "^5.3.2",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1",
        "openai": "^4.20.1",
        "pg": "^8.16.3",
        "posthog-node": "^3.1.2",
        "shopify-api-node": "^3.12.6",
        "stripe": "^14.10.0",
        "twilio": "^4.23.0",
        "winston": "^3.11.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "eslint": "^8.56.0",
        "prettier": "^3.1.1",
        "prisma": "^5.7.1"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "prisma": {
        "schema": "./prisma/schema.prisma"
    }
}