{
    "version": 2,
    "builds": [{
        "src": "src/server.js",
        "use": "@vercel/node"
    }],
    "routes": [{
            "src": "/api/whatsapp/(.*)",
            "dest": "/api/whatsapp/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/src/server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}