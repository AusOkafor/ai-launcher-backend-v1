{
    "version": 2,
    "builds": [{
            "src": "api/consolidated.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [{
            "src": "/api/(.*)",
            "dest": "/api/consolidated?path=$1"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "build": {
        "env": {
            "ENABLE_EXPERIMENTAL_COREPACK": "1"
        }
    },
    "functions": {
        "api/**/*.js": {
            "maxDuration": 30
        }
    }
}