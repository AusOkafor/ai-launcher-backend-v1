{
    "version": 2,
    "builds": [{
            "src": "api/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [{
            "src": "/api/whatsapp/(.*)",
            "dest": "/api/whatsapp?path=$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/src/server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "build": {
        "env": {
            "ENABLE_EXPERIMENTAL_COREPACK": "1"
        }
    },
    "functions": {
        "api/**/*.js": {
            "maxDuration": 30
        }
    }
}